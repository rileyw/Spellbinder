;Trigger File for Dwarven Halls


[triggerdefs]
numtriggers= 40

; ELEVATOR EXAMPLE
[trigger01]
type=elevator
texture_off=40     ; Texture when trigger is OFF
texture_on=39      ; Texture when trigger is ON
reset_timer=6000   ; Time in msec to reset from ON to OFF automatically
enabled=1          ; Trigger is Enabled
initial_state=0    ; Initial state -> 0=OFF 1=ON
next_trigger=0     ; Next trigger in chain
off_height=0       ; Height of floor when OFF
on_height=399      ; Height of floor when ON
x1=38              ; Upper left corner of elevator
y1=61
x2=38              ; Lower right corner of elevator
y2=61
speed=100          ; Speed of elevator in Height units per second
move_floor= 1      ;
move_ceiling=0     ; Move ceiling with floor?
move_rooftop=0     ; Move rooftop with floor?

