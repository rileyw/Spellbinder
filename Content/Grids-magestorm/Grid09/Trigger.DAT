[triggerdefs]
numtriggers=16

; ELEVATOR EXAMPLE
[trigger01]
type=elevator
texture_off=20     ; Texture when trigger is OFF
texture_on=21     ; Texture when trigger is ON
reset_timer=6000   ; Time in msec to reset from ON to OFF automatically
enabled=0          ; Trigger is Enabled
initial_state=0    ; Initial state -> 0=OFF 1=ON
next_trigger=0     ; Next trigger in chain
off_height=164       ; Height of floor when OFF
on_height=364      ; Height of floor when ON
x1=0              ; Upper left corner of elevator
y1=0
x2=0              ; Lower right corner of elevator
y2=0
speed=100          ; Speed of elevator in Height units per second
move_floor= 1      ;
move_ceiling=0     ; Move ceiling with floor?
move_rooftop=0     ; Move rooftop with floor?

